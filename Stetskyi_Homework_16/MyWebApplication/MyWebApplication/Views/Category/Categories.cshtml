@{
    ViewData["Title"] = "Categories Page";
}
@model List<Category>
<center><h1>@ViewData["Title"]</h1></center>


<u>Categories:</u>
<body>
    <p>
        <table>
            @foreach (Category cat in Model)
            {
                <tr>
                    <td>@cat.CategoryName</td>
                </tr>
            }
        </table>
    </p>

</body>
